#:kivy

<MoodSelect>:
    canvas.before:
        Color:
            rgba: 0.95, 0.95, 0.95, 1
        Rectangle:
            pos: self.pos
            size: self.size

<Button>:
    background_color: 0.9, 0.9, 0.9, 1
    color: 0.2, 0.2, 0.2, 1
    font_size: 16
    
<TextInput>:
    background_color: 1, 1, 1, 1
    foreground_color: 0.2, 0.2, 0.2, 1
    padding: 10